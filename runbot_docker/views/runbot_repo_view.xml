<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_runbot_docker_repo_form" model="ir.ui.view">
        <field name="model">runbot.repo</field>
        <field name="inherit_id" ref="runbot.repo_form"/>
        <field name="arch" type="xml">
        <xpath expr="//group[@name='repo_group']" position="after">
            <group name="runbot_docker">
                <field name="is_docker_image"/>
            </group>
        </xpath>
        </field>
    </record>
</odoo>
